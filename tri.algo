ALGORITHM tri
VAR
    t : integer
    i : integer
    j : integer
    k : integer
BEGIN
    j←2
    while j<=longueur(t):   //loop 1
        i←j-1
        k←t[j]
    while i>0 et que t[i]>k:   //loop 2
        t[i+1]←t[i]
        i←i-1
    end_while
        t[i+1]←k
        j←j+1
END